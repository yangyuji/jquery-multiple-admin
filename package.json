{
  "name": "jquery-multiple-admin",
  "version": "1.0.0",
  "description": "A Multi Page Application base on webpack4 + babel + sass + jquery + layui",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.conf.js ",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.conf.js",
    "server": "live-server ./ --port=8888"
  },
  "author": "yangyuji",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^8.5.0",
